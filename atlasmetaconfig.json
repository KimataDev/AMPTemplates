[
	{
        "ConfigFile": "AtlasTools/RedisDatabase/redis.conf",
        "ConfigType": "kvp",
        "ConfigFormat": "{0} {1}",
		"ConfigFormatRegex":"^(?:# )?(?<key>.+?) ?(?<value>.*?)$",
        "Subsections": [
            {
				"Heading": "$root",
				"SettingMappings":
					{
						"port": "$RedisPort",
						"requirepass": "$RedisPassword"
					}
            }
        ]
    },
	{
		"ConfigFile": "ShooterGame/ServerGrid.ServerOnly.json",
		"AutoMap": true,
		"ConfigType": "template",
		"ConfigFileTemplate": "atlas/AMP_ServerGrid.ServerOnly.json"
	}
]